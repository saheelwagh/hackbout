#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules/nodemon/bin/nodemon.js/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules/nodemon/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules/nodemon/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/nodemon/bin/nodemon.js/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/nodemon/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/nodemon/node_modules" "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules/nodemon/bin/nodemon.js/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules/nodemon/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules/nodemon/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/1.19.4/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/nodemon/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/nodemon/bin/nodemon.js/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/nodemon/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/nodemon/node_modules" node  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
fi
exit $ret

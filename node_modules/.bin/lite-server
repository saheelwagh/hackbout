#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules/lite-server/bin/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules/lite-server/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/lite-server/bin/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/lite-server/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/lite-server/node_modules" "$basedir/node"  "$basedir/../lite-server/bin/lite-server" "$@"
  ret=$?
else 
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules/lite-server/bin/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules/lite-server/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/2.5.4/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/lite-server/bin/lite-server/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/lite-server/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/lite-server/node_modules" node  "$basedir/../lite-server/bin/lite-server" "$@"
  ret=$?

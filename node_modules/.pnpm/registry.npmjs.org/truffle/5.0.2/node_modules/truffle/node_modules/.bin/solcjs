#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/0.5.0/node_modules/solc/solcjs/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/0.5.0/node_modules/solc/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/0.5.0/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../solc/0.5.0/node_modules/solc/solcjs" "$@"
  ret=$?
else 
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/0.5.0/node_modules/solc/solcjs/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/0.5.0/node_modules/solc/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/0.5.0/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/solc/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../solc/0.5.0/node_modules/solc/solcjs" "$@"
  ret=$?
fi
exit $ret

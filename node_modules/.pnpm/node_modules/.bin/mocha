#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules/mocha/bin/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules/mocha/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules/mocha/bin/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules/mocha/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules/mocha/node_modules" "$basedir/node"  "$basedir/../mocha/bin/mocha" "$@"
  ret=$?
else 
  NODE_PATH="/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules/mocha/bin/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules/mocha/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/4.1.0/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/registry.npmjs.org/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules:/home/saheelwagh/coding/hackathon/node_modules:/home/saheelwagh/coding/node_modules:/home/saheelwagh/node_modules:/home/node_modules:/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules/mocha/bin/mocha/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules/mocha/bin/node_modules:/home/saheelwagh/coding/hackathon/hackbout/node_modules/.pnpm/node_modules/mocha/node_modules" node  "$basedir/../mocha/bin/mocha" "$@"
  ret=$?
fi
exit $ret
